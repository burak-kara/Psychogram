{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "overrides": [
        {
            "files": "*.scss",
            "options": {
                "tabWidth": 4,
                "singleQuote": true
            }
        },
        {
            "files": "*.json",
            "options": {
                "tabWidth": 4,
                "singleQuote": true
            }
        }
    ]
}
